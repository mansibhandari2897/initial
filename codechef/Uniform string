#include<iostream>
using namespace std;
int main()
{int t,x;cin>>t;string s;
for(int i=0;i<t;i++)
    {cin>>s;
    x = 0;
    for(int j=0;j<7;j++)
       {
        if (s[j+1] != s[j])
            x++;}
    if (s[7] != s[0])
        x++;
    if(x <= 2)
        cout<<"uniform"<<endl;
    else
        cout<<"non-uniform"<<endl;
}
return 0;
}
