#include<iostream>
using namespace std;
int main()
{
    int t,n,s,x;cin>>t;
    while(t--)
    {
        cin>>n;s=0;
        for(int i=0;i<n;i++)
        {
            cin>>x;
            s=s+x;
        }
        if(s%2==0)
            cout<<"NO"<<endl;
        else
            cout<<"YES"<<endl;
    }
}
