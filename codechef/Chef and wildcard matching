#include<iostream>
using namespace std;
int main()
{
    int t,l,i;cin>>t;string s1,s2;
    while(t>0)
    {
        cin>>s1;
        cin>>s2;l=s1.length();
        for(i=0;i<l;i++)
        {
            if(s1[i]!=s2[i] && s1[i]!='?' && s2[i]!='?')
                break;
        }
        if(i==l)
            cout<<"Yes"<<endl;
        else
            cout<<"No"<<endl;
        t--;

    }
}
