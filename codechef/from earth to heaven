#include<iostream>
#include<math.h>
using namespace std;
int main()
{
    double t,n,v1,v2,t1,t2;cin>>t;
    while(t--)
    {
        cin>>n>>v1>>v2;
        t1=(sqrt(2)*n)/v1;
        t2=(2*n)/v2;
        if(t1<t2)
            cout<<"Stairs"<<endl;
        else
            cout<<"Elevator"<<endl;
    }

}
