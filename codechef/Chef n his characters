t=int(input())
for j in range(t):
    s=input()
    c=0
    for i in range(len(s)-3):
        ls=[]
        for k in range(i,i+4):
           d=s[k]
           ls.append(d)
        ls.sort()
        st=''.join(ls)
        if(st=="cefh"):
            c=c+1
    if c==0:
        print("normal \n")
    else:
        print("lovely",c,"\n")
