#include<iostream>
using namespace std;
int main()
{
    int t,n,m;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        cin>>n>>m;
        char a[n][m];
        for(int j=0;j<n;j++)
        {
            for(int k=0;k<m;k++)
                cin>>a[j][k];
        }
        cout<<a[n-1][m-1]<<endl;
    }
}
