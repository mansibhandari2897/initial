#include<iostream>
using namespace std;
int main()
{long t;
cin>>t;
while(t>0)
{
    int a;int k;
    cin>>a;k=a%12;
    if(k==1)
        cout<<a+11<<" "<<"WS"<<endl;
    else
        if(k==2)
        cout<<a+9<<" "<<"MS"<<endl;
    else
        if(k==3)
        cout<<a+7<<" "<<"AS"<<endl;
    else
        if(k==4)
        cout<<a+5<<" "<<"AS"<<endl;
    else
        if(k==5)
        cout<<a+3<<" "<<"MS"<<endl;
    else if(k==6)
        cout<<a+1<<" "<<"WS"<<endl;
        else
        if(k==7)
        cout<<a-1<<" "<<"WS"<<endl;
        else
        if(k==8)
        cout<<a-3<<" "<<"MS"<<endl;
        else
        if(k==9)
        cout<<a-5<<" "<<"AS"<<endl;
        else if(k==10)
        cout<<a-7<<" "<<"AS"<<endl;
        else if(k==11)
        cout<<a-9<<" "<<"MS"<<endl;
        else if(k==0)
        cout<<a-11<<" "<<"WS"<<endl;
        t--;
}
        return 0;
 
}
Language: C++14
