#include<iostream>
#include<string>
using namespace std;
int main()
{
    string str;
    cin>>str;int i,j,c=0;
    int l=str.length();
    for(i=0,j=l-1;i<l/2;i++,j--)
    {
        if(str[i]!=str[j])
            c=1;
    }
    if(c==1)
        cout<<"NO";
    else
        cout<<"YES";
    return 0;

}
