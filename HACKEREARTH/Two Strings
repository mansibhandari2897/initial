#include<iostream>
#include <algorithm>
#include<string>
using namespace std;
int main()
{
    int t;cin>>t;
    for(int i=0;i<t;i++)
    {
string str1,str2;
cin>>str1>>str2;
sort(str1.begin(),str1.end());
sort(str2.begin(),str2.end());
if(str1.compare(str2))
    cout<<"NO"<<endl;
else cout<<"YES"<<endl;}
     return 0;
}
Language: C++
